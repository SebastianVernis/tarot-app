{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    },
    {
      "src": "public/tarot_web.html",
      "use": "@vercel/static"
    },
    {
      "src": "public/tarot_web.js",
      "use": "@vercel/static"
    }
  },
  "redirects": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.py"
    },
    {
      "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|svg|ico|json))",
      "dest": "/public/$1"
    },
    {
      "src": "/",
      "dest": "/public/tarot_web.html"
    },
    {
      "src": "/(.*)",
      "dest": "/public/tarot_web.html"
    }
  ],
  "env": {
    "FLASK_ENV": "production",
    "PYTHONPATH": "/var/task",
    "PYTHON_VERSION": "3.11"
  },
  "regions": ["iad1"]
}